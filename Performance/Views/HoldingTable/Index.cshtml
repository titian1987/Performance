@model IEnumerable<Performance.Data.Entity.HoldingTable>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SecurityId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SecurityName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PortfolioId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PortfolioName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModelPortfolioId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModelPortfolioName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nominal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MarketValue)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CashFlow)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SecurityId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SecurityName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PortfolioId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PortfolioName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModelPortfolioId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModelPortfolioName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Price)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nominal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MarketValue)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CashFlow)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>

<p>@Html.ActionLink("Show", "Show")</p>